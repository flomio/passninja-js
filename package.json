{
  "name": "@flomio/passninja-js",
  "version": "1.0.0",
  "description": "PassNinja pass creation helper library.",
  "main": "dist/passninja.js",
  "module": "dist/passninja.esm.js",
  "browser": "dist/passninja.umd.js",
  "jsnext:main": "dist/passninja.esm.js",
  "types": "types/index.d.ts",
  "typings": "types/index.d.ts",
  "scripts": {
    "test": "yarn lint && yarn test:unit && yarn test:versions && yarn test:types && yarn typecheck",
    "test:unit": "jest",
    "test:types": "tsc -p ./tests/types && jest --roots '<rootDir>/tests/types'",
    "lint": "eslint '{src,types}/**/*.{ts,js}' && yarn prettier-list-different",
    "typecheck": "tsc",
    "build": "yarn clean && yarn rollup -c",
    "clean": "rimraf dist",
    "prettier": "prettier './**/*.{js,ts,md,html,css}' --write",
    "prettier-list-different": "prettier './**/*.{js,ts,md,html,css}' --list-different"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flomio/passninja-js.git"
  },
  "keywords": [
    "passninja",
    "passninja-js",
    "passninja.js",
    "pass",
    "apple",
    "google",
    "wallet",
    "sdk"
  ],
  "author": "PassNinja (https://www.passninja.com)",
  "license": "GPL-3.0-only",
  "bugs": {
    "url": "https://github.com/flomio/passninja-js/issues"
  },
  "homepage": "https://www.passninja.com",
  "files": [
    "dist"
  ],
  "devDependencies": {
    "@babel/core": "^7.11.4",
    "@babel/preset-env": "^7.11.0",
    "@rollup/plugin-babel": "^5.2.0",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^5.0.2",
    "@types/jest": "^26.0.10",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^3.10.1",
    "@typescript-eslint/parser": "^3.10.1",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.3.0",
    "eslint": "^7.7.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jest": "^23.20.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.4.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.26.5",
    "rollup-plugin-node-builtins": "^2.1.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-node-polyfills": "^0.2.1",
    "rollup-plugin-terser": "^7.0.0",
    "ts-jest": "^26.2.0",
    "typescript": "^4.0.2"
  }
}
